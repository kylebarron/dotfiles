"*":
  Hydrogen:
    autoScroll: false
    gateways: '''
      [
          {
              "name": "Remote",
              "options": {
                  "baseUrl": "http://localhost:8888",
                  "token": "redacted"
              }
          }
      ]
    '''
    languageMappings: "{\"ir\": \"r\", \"bash\": \"shell script\"}"
    startDir: "dirOfFile"
  "Remote-FTP":
    beta: {}
    connector:
      autoUploadOnSave: "only when connected"
    notifications:
      enableTransfer: true
      enableWatchFileChange: true
    tree:
      hideLocalWhenDisplayed: true
      useDockIntegration: true
  Zen:
    minimap: true
  aligner:
    alignComments: true
  "atom-alignment":
    addSpacePostfix: true
    alignBy: [
      "=>"
      ":="
      ":"
      "="
      "\""
      "|"
      "//"
      "/"
    ]
    alignmentSpaceChars: [
      "=>"
      ":="
    ]
  "atom-beautify":
    executables:
      rscript:
        path: "/usr/bin/Rscript"
    general: {}
    markdown:
      default_beautifier: "Remark"
    python:
      default_beautifier: "yapf"
      multi_line_output: "Grid"
  "atom-clock":
    dateFormat: "ddd MMM D YYYY h:mm A"
  "atom-ide-ui":
    use:
      "atom-ide-diagnostics-ui": false
  "atom-material-ui":
    colors:
      abaseColor: "#ffa726"
      accentColor: "#9f6918"
      paintCursor: true
      predefinedColor: "Orange"
    tabs:
      compactTabs: true
    ui:
      panelContrast: true
      panelShadows: true
  "atom-shell-commands": {}
  "atom-terminal": {}
  "autocomplete-bibtex":
    references: []
    resultTemplate: "\\cite{[key]}"
    scope: ".text.tex.latex,.source.gfm,.text.md"
  "autocomplete-paths": {}
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  "autocomplete-python":
    useKite: false
  "command-palette":
    preserveLastSearch: true
  core:
    closeDeletedFileTabs: true
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "welcome"
      "linter-flake8"
      "aligner"
      "aligner-javascript"
      "aligner-python"
      "language-pfm"
      "highlight-line"
      "autocomplete-R"
      "language-gfm"
      "minimap-cursorline"
      "linter-ui-default"
      "latexer"
      "julia-client"
      "ink"
      "atom-ide-ui"
      "teletype"
      "markdown-preview"
      "auto-detect-indentation"
      "python-tools"
      "autocomplete-python"
      "indent-guide-improved"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "data"
    ]
    packagesWithKeymapsDisabled: [
      "platformio-ide-terminal"
    ]
    projectHome: "/home/kyle/"
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
    warnOnLargeFileLimit: 5
  docblockr: {}
  editor:
    autoIndentOnPaste: false
    fontSize: 13
    preferredLineLength: 100
    scrollPastEnd: true
    softWrap: true
    softWrapHangingIndent: 4
    tabLength: 4
  "exception-reporting":
    userId: "1cb5eec8-c5ea-4294-a0a3-2d113b96890d"
  "file-icons": {}
  firacode:
    scopes: [
      "*"
    ]
  fonts:
    fontFamily: "FiraCode"
  "grammar-token-limit":
    maxTokensPerLine: 150
  "highlight-line":
    underline: "dashed"
  "highlight-selected":
    hideHighlightOnSelectedWord: true
  "hydrogen-launcher":
    command: "zsh"
  latex:
    outputDirectory: "build/"
  linter: {}
  "linter-flake8":
    ignoreErrorCodes: [
      "E221"
      "E251"
    ]
  "linter-ui-default":
    panelHeight: 61
    showPanel: true
  "markdown-preview-enhanced":
    codeBlockTheme: "atom-material.css"
    previewTheme: "atom-material.css"
  "markdown-preview-plus":
    enableLatexRenderingByDefault: true
    enablePandoc: true
    useLazyHeaders: false
  "markdown-table-formatter":
    limitLastColumnPadding: true
    markdownGrammarScopes: [
      "source.gfm"
      "text.md"
    ]
  "markdown-writer":
    fileExtension: ".md"
    relativeImagePath: true
    siteEngine: "jekyll"
    siteImagesDir: "docs/img/{year}/{month}/"
    tableExtraPipes: true
  minimap:
    adjustAbsoluteModeHeight: true
    plugins:
      bookmarks: true
      bookmarksDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      "minimap-autohide": false
      "minimap-autohideDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  "package-generator":
    packageSyntax: "coffeescript"
  "pdf-view":
    fitToWidthOnOpen: true
    paneToUseInSynctex: "right"
    reverseSyncBehaviour: "Double click"
  "platformio-ide-terminal":
    ansiColors:
      normal:
        black: "#252525"
        blue: "#40c4ff"
        cyan: "#18ffff"
        green: "#c3d82c"
        magenta: "#ff4081"
        red: "#ff5252"
        white: "#f5f5f5"
        yellow: "#ffd740"
    core:
      mapTerminalsTo: "File"
    customTexts: {}
    style:
      defaultPanelHeight: "15%"
    toggles:
      autoClose: true
  "project-manager":
    savePathsRelativeToHome: true
    sortBy: "last modified"
  "python-indent": {}
  "regex-railroad-diagram":
    enabled: false
  script:
    cwdBehavior: "Directory of the script"
  "spell-check":
    addKnownWords: true
    grammars: [
      "text.tex.latex"
      "text.md"
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "split-diff":
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
  "stata-exec":
    advancePosition: true
  tabs:
    enableMruTabSwitching: false
    enableVcsColoring: true
  "tree-view":
    hideVcsIgnoredFiles: true
  typewriter:
    showGutter: true
    showScrollbar: true
  wakatime:
    apikey: "Saved in your ~/.wakatime.cfg file"
    showStatusBarIcon: false
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
    ignoreWhitespaceOnlyLines: true
".coffee.source":
  editor:
    tabLength: 2
".md.text":
  editor:
    preferredLineLength: 100
    softWrapAtPreferredLineLength: true
".python.regexp.source":
  editor:
    preferredLineLength: 90
".python.source":
  editor:
    autoIndent: true
    preferredLineLength: 80
".r.source":
  "bracket-matcher":
    autocompleteCharacters: [
      "()"
      "[]"
      "{}"
      "\"\""
      "''"
      "``"
      "“”"
      "‘’"
      "«»"
      "‹›"
    ]
  "wrap-guide":
    enabled: false
".regexp.source.stata":
  editor:
    preferredLineLength: 150
".source.stata":
  "atom-alignment":
    addSpacePostfix: true
    alignBy: [
      "%"
      "=>"
      ":="
      ":"
      "="
      "\""
      "\\"
      "`"
      ">="
      "<="
      "/"
    ]
    alignmentSpaceChars: [
      "=>"
      ":="
      ":"
    ]
  editor:
    autoIndent: true
    autoIndentOnPaste: true
    preferredLineLength: 150
    showIndentGuide: true
    showInvisibles: false
    softWrapAtPreferredLineLength: false
"text.latex.tex":
  "atom-alignment":
    addSpacePostfix: true
    alignBy: [
      "%"
      "&"
    ]
    alignmentSpaceChars: [
      "=>"
      ":="
      ":"
    ]
  editor:
    tabLength: 4
"text.tex":
  "atom-alignment":
    addSpacePostfix: true
    alignBy: [
      "%"
    ]
    alignmentSpaceChars: [
      "=>"
      ":="
      ":"
    ]
